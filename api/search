-berlow":2,"marc-foley":2,"sponsorship":6,"support":27,"key":4,"operating-systems":4,"browsers":6,"font-source-formats":3,"font-editors":6,"are-we-ready-for-avar2":2}},"avgFieldLength":{"content":17.213286713286696,"page_id":2,"type":1,"breadcrumbs":2,"url":2.818181818181819},"fieldLengths":{"content":{"1":2,"2":18,"3":4,"4":2,"5":2,"6":2,"7":2,"8":1,"9":59,"10":10,"11":20,"12":15,"13":54,"14":45,"15":35,"16":8,"17":51,"18":21,"19":22,"20":5,"21":6,"22":7,"23":5,"24":7,"25":1,"26":15,"27":2,"28":2,"29":7,"30":12,"31":2,"32":14,"33":6,"34":7,"35":2,"36":11,"37":3,"38":3,"39":6,"40":1,"41":11,"42":2,"43":2,"44":2,"45":2,"46":2,"47":1,"48":37,"49":34,"50":22,"51":29,"52":26,"53":15,"54":46,"55":21,"56":29,"57":36,"58":15,"59":21,"60":15,"61":30,"62":38,"63":26,"64":24,"65":5,"66":5,"67":8,"68":3,"69":13,"70":17,"71":56,"72":24,"73":18,"74":10,"75":24,"76":34,"77":22,"78":39,"79":50,"80":33,"81":37,"82":27,"83":5,"84":10,"85":8,"86":7,"87":5,"88":5,"89":1,"90":16,"91":1,"92":2,"93":2,"94":1,"95":1,"96":1,"97":4,"98":3,"99":4,"100":4,"101":2,"102":1,"103":1,"104":2,"105":2,"106":1,"107":20,"108":34,"109":21,"110":41,"111":13,"112":47,"113":15,"114":35,"115":13,"116":47,"117":59,"118":22,"119":90,"120":10,"121":23,"122":40,"123":9,"124":55,"125":47,"126":19,"127":36,"128":53,"129":59,"130":61,"131":50,"132":22,"133":31,"134":25,"135":83,"136":56,"137":41,"138":59,"139":10,"140":29,"141":19,"142":38,"143":53,"144":25,"145":35,"146":25,"147":48,"148":41,"149":30,"150":14,"151":17,"152":30,"153":26,"154":8,"155":5,"156":9,"157":7,"158":8,"159":8,"160":7,"161":9,"162":8,"163":7,"164":8,"165":7,"166":7,"167":12,"168":4,"169":8,"170":6,"171":6,"172":12,"173":7,"174":5,"175":7,"176":7,"177":3,"178":7,"179":5,"180":8,"181":8,"182":6,"183":7,"184":5,"185":8,"186":10,"187":5,"188":6,"189":5,"190":7,"191":5,"192":7,"193":5,"194":5,"195":7,"196":8,"197":9,"198":9,"199":10,"200":7,"201":8,"202":2,"203":13,"204":4,"205":3,"206":1,"207":21,"208":58,"209":64,"210":47,"211":12,"212":36,"2