-fonts"],[249,"/docs/parametric-fonts"],[250,"/docs/parametric-fonts"],[251,"/docs/parametric-fonts"],[252,"/docs/parametric-fonts"],[253,"/docs/parametric-fonts"],[254,"/docs/sponsorship"],[255,"/docs/sponsorship"],[256,"/docs/sponsorship"],[257,"/docs/sponsorship"],[258,"/docs/sponsorship"],[259,"/docs/sponsorship"],[260,"/docs/support"],[261,"/docs/support"],[262,"/docs/support"],[263,"/docs/support"],[264,"/docs/support"],[265,"/docs/support"],[266,"/docs/support"],[267,"/docs/support"],[268,"/docs/support"],[269,"/docs/support"],[270,"/docs/support"],[271,"/docs/support"],[272,"/docs/support"],[273,"/docs/support"],[274,"/docs/support"],[275,"/docs/support"],[276,"/docs/support"],[277,"/docs/support"],[278,"/docs/support"],[279,"/docs/support"],[280,"/docs/support"],[281,"/docs/support"],[282,"/docs/support"],[283,"/docs/support"],[284,"/docs/support"],[285,"/docs/support"],[286,"/docs/support"]],"type":"string"},"type":{"docs":{"1":44,"2":53,"3":0,"4":1,"5":2,"6":3,"7":4,"8":5,"9":54,"10":55,"11":56,"12":57,"13":58,"14":59,"15":60,"16":61,"17":62,"18":63,"19":64,"20":65,"21":66,"22":67,"23":68,"24":69,"25":45,"26":70,"27":6,"28":71,"29":72,"30":73,"31":74,"32":75,"33":76,"34":77,"35":78,"36":79,"37":80,"38":81,"39":82,"40":46,"41":83,"42":7,"43":8,"44":9,"45":10,"46":11,"47":12,"48":84,"49":85,"50":86,"51":87,"52":88,"53":89,"54":90,"55":91,"56":92,"57":93,"58":94,"59":95,"60":96,"61":97,"62":98,"63":99,"64":100,"65":101,"66":102,"67":103,"68":47,"69":104,"70":105,"71":106,"72":107,"73":108,"74":109,"75":110,"76":111,"77":112,"78":113,"79":114,"80":115,"81":116,"82":117,"83":118,"84":119,"85":120,"86":121,"87":122,"88":123,"89":48,"90":124,"91":13,"92":14,"93":15,"94":16,"95":17,"96":18,"97":19,"98":20,"99":21,"100":22,"101":23,"102":24,"103":25,"104":26,"105":27,"106":28,"107":125,"108":126,"109":127,"110":128,"111":129,"112":130,"113":131,"114":132,"115":133,"116":134,"117":135,"118":136,"119":137,"120":138,"121":139,"122":140,"123":141,"124":142,"125":143,"